{
	"name": "@mozisan/prettier-config",
	"version": "2.3.0",
	"homepage": "https://github.com/mozisan/prettier-config#readme",
	"bugs": {
		"url": "https://github.com/mozisan/prettier-config/issues"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/mozisan/prettier-config.git"
	},
	"license": "MIT",
	"author": "mozisan",
	"exports": {
		".": "./lib/default.cjs",
		"./tailwindcss": "./lib/tailwindcss.cjs"
	},
	"main": "./lib/tailwindcss.cjs",
	"files": [
		"lib",
		"package.json"
	],
	"scripts": {
		"prepare": "husky install",
		"fix": "run-s fix:eslint fix:prettier",
		"fix:eslint": "eslint --fix .",
		"fix:prettier": "prettier --write --log-level=warn .",
		"lint": "run-s lint:eslint lint:prettier",
		"lint:eslint": "eslint .",
		"lint:prettier": "prettier --check --log-level=warn ."
	},
	"prettier": "./lib/tailwindcss.cjs",
	"dependencies": {
		"prettier-plugin-packagejson": "^2.4.5",
		"prettier-plugin-tailwindcss": "^0.4.1"
	},
	"devDependencies": {
		"@mozisan/eslint-plugin": "^2.0.0",
		"eslint": "^8.46.0",
		"husky": "^8.0.3",
		"lint-staged": "^13.2.3",
		"npm-run-all": "^4.1.5",
		"prettier": "^3.0.0"
	},
	"peerDependencies": {
		"prettier": "^3"
	},
	"packageManager": "pnpm@8.6.9",
	"publishConfig": {
		"access": "public"
	}
}
